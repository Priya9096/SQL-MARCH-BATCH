create table Emp(
Emp_id int UNIQUE,
emp_name varchar(20) NOT NULL
);

insert into Emp values
(101,'Akash'),(102,'Pooja'),(103,'Suresh'),
(104,'Prakash'),(105,'Shreya'),(null,'Ankit'),
(null,'Ayush');

select * from EMP;

-- In Unique Constraint column only unique values are allowed but 
--it also accepts null values:
-- null == null ---> null
-- null + null ---> null
-- null + 5 ---> null

